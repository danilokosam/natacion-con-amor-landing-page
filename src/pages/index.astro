---
import Layout from "../layouts/Layout.astro";
import HeroIndex from "../components/homepage/HeroIndex.astro";
import { getCollection } from "astro:content";
import Contact from "../components/porfolio/Contact.astro";
import ListPricing from "../components/porfolio/ListPricing.astro";
import Schedule from "../components/ui/Schedule";
import Carousel from "../components/ui/Carousel";

const [staticData] = await getCollection("staticData");
const { pageTitle, pageDescription } = staticData.data;
---

<Layout pageTitle={pageTitle} description={pageDescription}>
  <HeroIndex />
  <ListPricing />
  <Schedule client:visible />
  <Carousel client:visible />
  <Contact />
</Layout>
